<div class="container">
    <div class="row d-flex justify-content-center">
      <div class="table-responsive">
        <table id="table"
        data-search="true"
        data-show-export="true"
        data-show-fullscreen="true"
        data-pagination="true"
        data-show-footer="true"
        data-show-pagination-switch="true"
        data-toggle="table"
        data-page-list="[10, 25, 50, 100, all]"
        data-search-highlight="true"
         class="table table-striped table-bordered table-hover fs-6 text-capitalize">
          <thead class="table-dark">
            <% for(let i in data){%>
            <tr>
              <% let value = data[i] %>
            
              <!-- inner loop for accessing headings -->
              <% for(let j in value){%>
              <th style="cursor:pointer" data-sortable="true"><%=j%></th>
              <%}%> <%break%>
            </tr>
            <%}%>
          </thead>
          <tbody class="table-success">
            <% for(let i in data){%>
            <tr>
              <% let value = data[i] %>
              <!-- inner loop for accessing headings -->
              <% for(let j in value){%>
              <td class="p-3"><%=value[j]%></td>
              <%}%>
            </tr>
            <%}%>
          </tbody>
        </table>
        </div>
      </div>
    </div>
  </div>